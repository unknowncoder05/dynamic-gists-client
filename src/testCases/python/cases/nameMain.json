{
    "requires":[
        {
            "from": "./helloWorld.json",
            "import": "*",
            "as":"hello_world"
        }
    ],
    "type":"code",
    "code":[
        "##:hello_world##",
        {
            "type":"if",
            "condition":"__name__ == '__main__'",
            "code":[
                {
                    "type":"functionCall",
                    "function":"##hello_world.renderInputs.functionName##"
                }
            ]
        }
    ]
}